KC_BOOTSTRAP_ADMIN_USERNAME=bootstrap
KC_BOOTSTRAP_ADMIN_PASSWORD=changeme
KC_DB=postgres
KC_DB_URL_HOST=keycloak-db
KC_DB_URL_DATABASE=keycloak
KC_DB_USERNAME=keycloak-db-user
KC_DB_PASSWORD=keycloak-db-password
# Keycloak 26 hostname v2: use KC_HOSTNAME (full URL). When behind nginx, compose uses
# --hostname-strict=false and --proxy-headers=xforwarded so URL is resolved from X-Forwarded-* headers.
KC_PROXY_HEADERS=xforwarded
KC_HOSTNAME=https://auth.eigentask.com
KC_PROXY_ADDRESS_FORWARDING=true
